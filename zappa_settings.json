{
  "dev": {
    "app_function": "app.app",
    "aws_region": "ap-southeast-1",
    "project_name": "agent-api",
    "runtime": "python3.11",
    "s3_bucket": "agent-api-zappa-deployments",
    "memory_size": 512,
    "timeout_seconds": 60,
    "exclude": [
      "*.pyc",
      "__pycache__",
      "*.git*",
      "venv/*",
      ".env",
      "tests/*",
      "*.md",
      ".github/*"
    ],
    "keep_warm": false,
    "cors": true,
    "api_key_required": false,
    "binary_support": true,
    "slim_handler": true
  },
  "production": {
    "app_function": "app.app",
    "aws_region": "ap-southeast-1",
    "project_name": "agent-api",
    "runtime": "python3.11",
    "s3_bucket": "agent-api-zappa-deployments-prod",
    "memory_size": 512,
    "timeout_seconds": 60,
    "exclude": [
      "*.pyc",
      "__pycache__",
      "*.git*",
      "venv/*",
      ".env",
      "tests/*",
      "*.md",
      ".github/*"
    ],
    "keep_warm": true,
    "cors": true,
    "api_key_required": false,
    "binary_support": true,
    "slim_handler": true
  }
}
